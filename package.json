{
  "name": "services-sy-mobile",
  "version": "1.0.0",
  "description": "تطبيق منصة الخدمات السورية للأندرويد و iOS",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.0",
    "axios": "^1.6.0",
    "expo": "^50.0.0",
    "expo-image-picker": "^17.0.10",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.11.1",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.27.0",
    "url-parse": "^1.5.10",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.4.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.0"
  },
  "expo": {
    "name": "منصة الخدمات السورية",
    "slug": "services-sy-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.servicesy.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.servicesy.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "السماح بالوصول إلى الصور",
          "cameraPermission": "السماح بالوصول إلى الكاميرا"
        }
      ]
    ]
  }
}
